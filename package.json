  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@dotcom-tool-kit/circleci": "^2.1.6",
    "@dotcom-tool-kit/circleci-npm": "^2.0.9",
